# Configuração simplificada sem Redis
DATABASE_URL="file:./dev.db"
PORT=3001
NODE_ENV="development"
FRONTEND_URL="http://localhost:3000"

# JWT
JWT_SECRET="dev_jwt_secret_key_change_in_production_123456789"
JWT_REFRESH_SECRET="dev_refresh_secret_key_change_in_production_987654321"
JWT_EXPIRES_IN="15m"
JWT_REFRESH_EXPIRES_IN="7d"

# Security
BCRYPT_ROUNDS=12
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL="info"

# Desabilitar workers que dependem do Redis
DISABLE_WORKERS=true